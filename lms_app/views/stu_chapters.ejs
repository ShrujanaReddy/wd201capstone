<h3>Chapters</h3>
<% for (var i=0; i < chapters.length; i++) { %>
    <h4>
        <%= chapters[i].title %>
    </h4>
    <p>
        <%= chapters[i].description %>
    </p>
    <% if (userIsEnrolled) { %>
        <a href="/students/<%= courseId %>/chapters/<%= chapters[i].id %>/pages" class="btn btn-primary">View
            Pages</a>
    <% } %>
<% } %>